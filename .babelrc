{
  "env": {
    "development": {
      "presets": [
        "es2015",
        "react",
        "env"
      ],
      "plugins": [
        "transform-runtime",
        [
          "import",
          {
            "libraryName": "antd",
            "style": true
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        "transform-runtime",
        [
          "import",
          {
            "libraryName": "antd",
            "style": true
          }
        ]
      ],
    }
  }
}